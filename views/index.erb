<!DOCTYPE html>
<html>

  <head>
    <title>FEED ME</title>
    <link rel="stylesheet" type="text/css" href="/css/reset.css">
     <link href='/css/lunch.css' rel='stylesheet'>
      <link href='//fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
  </head>

  <body>
    <section>
      <!-- <button data-option='list' id="list_button">List</button> -->
      <!-- <button data-option='map' id="map_button">Map</button> -->
      <aside id='navigation'>
        <img src="/images/feedme.png" alt="Feed me Logo">
        <nav>
          <ul><!-- button category template rendered here--></ul>
        </nav>
        <footer>
          Created by <a href="https://twitter.com/chloe_does">Chloe Do.es</a> &
          <a href="https://twitter.com/ecomba">Mr Ripenhausen</a>
        </footer>
      </aside>
      <section id='map'></section>
      <section id='results'></section>
    </section>
    <footer>
    </footer>
    <template id='profile_template'>
      <article>
        <h1><a href="{{url}}" target="_blank">{{venue.name}}</a></h1>
        <p>{{venue.price.message}} | {{venue.location.distance}}m from you | average rating: {{venue.rating}}</p>
        <address>
          <a target="_blank" href="http://citymapper.com/directions?startcoord={{currentLocation.latitude}},{{currentLocation.longitude}}&startname=Current+Location&startaddress=Current+Location&endcoord={{venue.location.lat}},{{venue.location.lng}}&endname={{venue.name}}&endaddress={{venue.location.address}}%20({{venue.location.crossStreet}})
          ">{{venue.location.address}} {{venue.location.postalCode}}</a>&nbsp;&nbsp;<a href="citymapper://directions?startcoord={{currentLocation.latitude}},{{currentLocation.longitude}}&startname=Current+Location&startaddress=Current+Location&endcoord={{venue.location.lat}},{{venue.location.lng}}&endname={{venue.name}}&endaddress={{venue.location.address}}%20({{venue.location.crossStreet}})
          "><img src="/images/citymapper.png"></a></address>
        <footer>{{venue.hours.status}}</footer>
      </article>
    </template>
    <template id='categories'>
      {{#categories}}
        <li>
          <a href='#' class="button" data-query='{{query}}'>{{name}}</a>
        </li>
      {{/categories}}
    </template>
    <script src='//code.jquery.com/jquery-2.0.3.min.js'></script>
    <script src='/js/mustache.js'></script>
    <script src='/js/underscore.js'></script>
    <script src="//maps.google.com/maps/api/js?sensor=true"></script>
    <script src='/js/gmaps.js'></script>
    <script src='/js/lunch_me.js'></script>
    <script src="/js/app.js"></script>
  </body>
</html>
