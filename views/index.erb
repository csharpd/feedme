<!DOCTYPE html>
<html>

  <head>
    <title>Lunch-Me</title>
    <link rel="stylesheet" type="text/css" href="/css/reset.css">
     <link href='/css/lunch.css' rel='stylesheet'>
      <link href='//fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
  </head>

  <body>
    <header>
      <h1>Lunch-Me!</h1>
    </header>
    <section>
      <aside>
        <nav>
          <ul><!-- button category template rendered here--></ul>
        </nav>
      </aside>
      <section id='map'></section>
      <section id='results'></section>
    </section>
    <footer>
      <button data-option='list' id="list_button">List</button>
      <button data-option='map' id="map_button">Map</button>
    </footer>
    <template id='profile_template'>
      <article>
        <h1>{{name}}</h1>
        <p id='rating'>{{rating}}</p>
        <h4>Cost: {{price.message}}</h4>
        <p>{{location.distance}} meters away</p>
        <p>{{location.formattedAddress.0}} </p>
        <p>{{location.formattedAddress.3}}</p>
      </article>"
    </template>
    <template id='categories'>
      {{#categories}}
        <li>
          <a href='#' class="button" data-query='{{query}}'>{{name}}</a>
        </li>
      {{/categories}}
    </template>
    <script src='//code.jquery.com/jquery-2.0.3.min.js'></script>
    <script src='/js/mustache.js'></script>
    <script src='/js/underscore.js'></script>
    <script src="//maps.google.com/maps/api/js?sensor=true"></script>
    <script src='/js/gmaps.js'></script>
    <script src='/js/lunch_me.js'></script>
    <script src="/js/app.js"></script>
  </body>
</html>
